{
  "name": "itty-durable",
  "version": "0.0.1",
  "description": "Cloudflare Durable Objects + Itty Router = Shorter Code",
  "main": "./dist/itty-durable.min.js",
  "types": "./dist/itty-durable.d.ts",
  "files": [
    "dist/itty-durable.js",
    "dist/itty-durable.min.js",
    "dist/itty-durable.d.ts"
  ],
  "keywords": [
    "router",
    "cloudflare",
    "workers",
    "serverless",
    "durable",
    "objects",
    "durableobjects",
    "api",
    "handler",
    "middleware",
    "durable",
    "objects"
  ],
  "scripts": {
    "lint": "npx eslint src",
    "test:dist": "jest --verbose --coverage dist",
    "test": "yarn lint && jest --verbose --coverage src",
    "verify": "echo 'verifying module...' && yarn build && yarn test && yarn test:dist",
    "dev": "yarn lint && jest --verbose --watch src",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prerelease": "yarn verify",
    "prebuild": "rimraf dist && mkdir dist && node prebuild.js && cp src/itty-durable.d.ts dist",
    "build": "terser -c -m toplevel -- dist/itty-durable.js  > dist/itty-durable.min.js",
    "postbuild": "node check-size.js",
    "release": "release --tag --push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwhitley/itty-durable.git"
  },
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwhitley/itty-durable/issues"
  },
  "homepage": "https://itty-durable.dev",
  "devDependencies": {
    "chalk": "^4.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-plugin-jest": "^24.3.4",
    "fs-extra": "^9.1.0",
    "gzip-size": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "itty-router": "^2.3.6",
    "itty-router-extras": "^0.2.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "terser": "^5.6.1",
    "terser-folder": "^2.0.0",
    "yarn-release": "^1.10.3"
  }
}
